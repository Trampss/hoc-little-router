{
  "name": "hoc-little-router",
  "version": "1.1.0",
  "description": "HOC to hide a component when the route didn't match. Edit",
  "license": "MIT",
  "main": "build/index.js",
  "contributors": [
    "Fabien JUIF <fabien.juif@gmail.com>",
    "Guillaume CRESPEL <guillaume.crespel@gmail.com>"
  ],
  "scripts": {
    "build": "cross-env BABEL_ENV=cjs babel --ignore \"*.spec.js\" ./src/ --out-dir build",
    "test": "jest",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint:js": "eslint --ext js,jsx src",
    "lint": "npm-run-all --parallel lint:*",
    "ci": "npm-run-all --parallel lint coveralls"
  },
  "peerDependencies": {
    "react": "^15.4.2 || ^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "coveralls": "^2.11.16",
    "cross-env": "^3.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "jest": "^22.1.4",
    "npm-run-all": "3.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.6.0",
    "sinon": "^1.17.7"
  },
  "jest": {
    "transform": {
      ".*": "./node_modules/babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupTestFrameworkScriptFile": "./misc/test-setup.js"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ],
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "plugins": [
      "babel"
    ],
    "globals": {
      "fetch": false
    },
    "env": {
      "browser": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "arrow-body-style": 0,
      "import/prefer-default-export": 0,
      "react/forbid-prop-types": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "func-names": 0,
      "jsx-a11y/no-static-element-interactions": 0
    }
  }
}
